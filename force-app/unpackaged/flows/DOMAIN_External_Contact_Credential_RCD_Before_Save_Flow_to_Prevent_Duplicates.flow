<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>64.0</apiVersion>
    <description>DOMAIN LAYER - External Contact Credential Object - Record Trigger - Before the Record is Saved Create the external Contact Credential Record&apos;s Unique Key and check to make sure it doesn&apos;t exist already.</description>
    <environments>Default</environments>
    <formulas>
        <name>UniqueKeyValue</name>
        <dataType>String</dataType>
        <expression>$Record.POE_Contact__c &amp; &apos;|&apos; &amp;$Record.Vendor__c&amp; &apos;|&apos; &amp;$Record.External_Contact_Credential_Type__c</expression>
    </formulas>
    <interviewLabel>DOMAIN - External Contact Credential - RCD - Before Safe Flow to Prevent Duplicates {!$Flow.CurrentDateTime}</interviewLabel>
    <label>DOMAIN - External Contact Credential - RCD - Before Save Flow to Prevent Duplicates</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>Create_Unique_Key</name>
        <label>Create Unique Key</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <inputAssignments>
            <field>Unique_Key__c</field>
            <value>
                <elementReference>UniqueKeyValue</elementReference>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Create_Unique_Key</targetReference>
        </connector>
        <object>POE_External_Contact_Credential__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordBeforeSave</triggerType>
    </start>
    <status>Active</status>
</Flow>
