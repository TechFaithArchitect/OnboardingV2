        <dataType>String</dataType>
        <expression>RIGHT({!Loop_and_Build_FinalRecords},LEN({!Loop_and_Build_FinalRecords}) - FIND(&quot;|&quot;,{!Loop_and_Build_FinalRecords}))</expression>
    </formulas>
