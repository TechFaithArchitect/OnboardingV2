        </defaultConnector>
        <defaultConnectorLabel>No - They Do Not Match</defaultConnectorLabel>
        <rules>
            <name>Yes_TA_and_ECC_Have_Same_Training_Requirement</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Loop_External_Contact_Credentials.Training_Requirement__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
