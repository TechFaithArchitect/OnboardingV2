            </value>
        </inputAssignments>
        <inputAssignments>
            <field>POE_Process_Status__c</field>
            <value>
                <stringValue>Complete</stringValue>
            </value>
        </inputAssignments>
        <object>POE_External_Contact_Credential__c</object>
    </recordUpdates>
    <start>
