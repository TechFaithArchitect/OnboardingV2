<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>64.0</apiVersion>
    <assignments>
        <name>Assign_Count_of_Related_Training_Assignments</name>
        <label>Assign Count of Related Training Assignments</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>TotalNumberOfRelatedTrainingAssignments</assignToReference>
            <operator>AssignCount</operator>
            <value>
                <elementReference>TrainingAssignmentRecordCollection</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>CompletedOrPassedTrainingAssignments</assignToReference>
            <operator>AssignCount</operator>
            <value>
                <elementReference>Filter_Related_Training_Assignments</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Determine_If_All_Courses_Passed_or_Completed</targetReference>
        </connector>
    </assignments>
    <collectionProcessors>
        <name>Filter_Related_Training_Assignments</name>
        <elementSubtype>FilterCollectionProcessor</elementSubtype>
        <label>Filter Related Training Assignments</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignNextValueToReference>currentItem_Filter_Related_Training_Assignments</assignNextValueToReference>
        <collectionProcessorType>FilterCollectionProcessor</collectionProcessorType>
        <collectionReference>TrainingAssignmentRecordCollection</collectionReference>
        <conditionLogic>or</conditionLogic>
        <conditions>
            <leftValueReference>currentItem_Filter_Related_Training_Assignments.Status__c</leftValueReference>
            <operator>EqualTo</operator>
            <rightValue>
                <stringValue>Completed</stringValue>
            </rightValue>
        </conditions>
        <conditions>
            <leftValueReference>currentItem_Filter_Related_Training_Assignments.Status__c</leftValueReference>
            <operator>EqualTo</operator>
            <rightValue>
                <stringValue>Passed</stringValue>
            </rightValue>
        </conditions>
        <conditions>
            <leftValueReference>currentItem_Filter_Related_Training_Assignments.Status__c</leftValueReference>
            <operator>EqualTo</operator>
            <rightValue>
                <stringValue>Exempt</stringValue>
            </rightValue>
        </conditions>
        <connector>
            <targetReference>Assign_Count_of_Related_Training_Assignments</targetReference>
        </connector>
    </collectionProcessors>
    <decisions>
        <name>Determine_If_All_Courses_Passed_or_Completed</name>
        <label>Determine If All Courses Passed or Completed</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Not All Courses Have Been Passed</defaultConnectorLabel>
        <rules>
            <name>All_Courses_Have_Been_Passed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>TotalNumberOfRelatedTrainingAssignments</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>CompletedOrPassedTrainingAssignments</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_External_Contact_Creation_Record</targetReference>
            </connector>
            <label>All Courses Have Been Passed</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Last_Updated_System_ECC</name>
        <label>Is Last Updated System ECC</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Is_Required_Credential_Record_Null</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No Last Updated System is Not ECC</defaultConnectorLabel>
        <rules>
            <name>Yes_Last_Update_System_is_ECC</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ExternalContactCredentialRecord.Last_Updated_By_System__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>ECC</stringValue>
                </rightValue>
            </conditions>
            <label>Yes Last Update System is ECC</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Required_Credential_Record_Null</name>
        <label>Is Required Credential Record Null</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Get_All_Related_Training_Assignments</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Required Credential Record is Not Null</defaultConnectorLabel>
        <rules>
            <name>Required_Credential_Record_is_Null</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ContactTrainingAssignmentRecord.Vendor_Training_Requirement__r.Required_Credential__r.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <label>Required Credential Record is Null</label>
        </rules>
    </decisions>
