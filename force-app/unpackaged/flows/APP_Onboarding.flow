            <label>Setup Complete</label>
        </rules>
        <rules>
            <name>Setup_Cancelled</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Onboarding_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Canceled</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Close_All_Onboarding_Tasks_Stage</targetReference>
            </connector>
            <label>Setup Cancelled</label>
        </rules>
        <rules>
            <name>Setup_Expired</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Onboarding_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Expired</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Close_All_Onboarding_Tasks_Stage</targetReference>
            </connector>
            <label>Setup Expired</label>
        </rules>
        <rules>
