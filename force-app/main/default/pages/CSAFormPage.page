<apex:page docType="html-5.0" controller="CSAFormPageController">

    <html lang="en" class="html-alignment">

    <head>
        <title>Residential Sales Order</title>
        <apex:stylesheet value="{!URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/css/csaFormPageStyle.css')}" />
        <script>
            function rerenderPage() {
                submitForm(); // Trigger the Visualforce ActionFunction for final refresh
            }
        </script>
    </head>

    <body>
        <apex:form id="form">
            <apex:actionFunction name="submitForm" rerender="main-form, submit-screen" />

            <!-- Main Form Panel, only visible when not submitted -->
            <apex:outputPanel id="main-form" layout="block" rendered="{!NOT(submitted)}">
                <div class="header-alignment">
                    <h1 class="header">
                        <span class="header-title-one">Residential Sales Order</span>
                        <span class="header-title-two">Thank you. We appreciate your business.</span>
                    </h1>
                </div>
                <table class="form-box main-table">
                    <!--   Header field set -->
                    <tr class="header-table-row">
                        <td class="header-table-data-cell" colspan="2">
                            <div class="s3 header-field-set">
                                <span style="width: 160pt; display: inline-block;">
                                    <label for="monthlyRecurringCharges">Monthly Recurring Charges: $</label>
                                    <apex:outputText value="{!monthlyRecurringCharges}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    <apex:inputText id="monthlyRecurringCharges" value="{!monthlyRecurringCharges}" size="9" required="true" />
                                </span>
                                <span style="width: 134pt; display: inline-block;">
                                    <label for="dueDate">Due Date: </label>
                                    <apex:outputText value="{!dueDate}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    <apex:input id="dueDate" value="{!dueDate}" type="date" />
                                </span>
                                <span style="width: 106pt; display: inline-block;">
                                    <label for="installationFee">Installation Fee $ </label>
                                    <apex:outputText value="{!installationFee}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    <apex:inputText id="installationFee" value="{!installationFee}" size="7" required="true" />
                                </span>
                                <span style="width: 123pt; display: inline-block;">
                                    <label for="arrivalWindow"> Arrival Window: </label>
                                    <apex:outputText value="{!arrivalWindow}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    <apex:inputText id="arrivalWindow" value="{!arrivalWindow}" size="12" />
                                </span>
                                <span id="radio-container" class="s6">
                                    <apex:selectRadio id="am-pm" value="{!arrivalWindowAmOrPm}" html-class="apex-radio-button" layout="lineDirection" rendered="{!NOT(isPdfMode)}">
                                        <apex:selectOptions value="{!timeOptions}" />
                                    </apex:selectRadio>
                                    <apex:outputText value="{!arrivalWindowAmOrPm}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                </span>

                            </div>
                            <p class="s6 header-sub-text">
                                The above pricing does not include taxes and surcharges.
                            </p>
                        </td>
                    </tr>
                    <!-- Section A -->
                    <tr class="section-title-table-row">
                        <td class="section-title-data-cell" colspan="2">
                            <p class="s7 section-header-text">
                                Section A: Customer Information (Must be filled out completely) PLEASE PRINT
                            </p>
                        </td>
                    </tr>
                    <tr class="section-a-table-row-one">
                        <td class="section-a-table-data-cell-one" colspan="2">
                            <div class="s3" style="display: block; text-align: left; line-height: 14pt; max-width: 575pt; margin-top:1pt;">
                                <!-- Two fields per row -->
                                <span class="section-a-input-spacing" style="width: 240pt;">
                                    <label for="btnPortIn">BTN/Port-In: </label>
                                    <apex:outputText value="{!btnPortIn}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    <apex:inputText id="btnPortIn" value="{!btnPortIn}" size="37" required="true" />
                                </span>
                                <span class="section-a-input-spacing" style="width: 308pt;">
                                    <label for="customerContactAddress">Customer contact email address: </label>
                                    <apex:outputText value="{!customerContactAddress}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    <apex:inputText id="customerContactAddress" value="{!customerContactAddress}" size="36" required="true" />
                                </span>
                                <span class="section-a-input-spacing" style="width: 266pt;">
                                    <label for="firstName">First Name: </label>
                                    <apex:outputText value="{!firstName}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    <apex:inputText id="firstName" value="{!firstName}" size="42" required="true" />
                                </span>
                                <span class="section-a-input-spacing" style="width: 285pt;">
                                    <label for="lastName">Last Name: </label>
                                    <apex:outputText value="{!lastName}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    <apex:inputText id="lastName" value="{!lastName}" size="45" required="true" />
                                </span>
                                <span class="section-a-input-spacing" style="width: 337pt;">
                                    <label for="preferredContactNumber">Preferred Contact # for date of activation (call/text): </label>
                                    <apex:outputText value="{!preferredContactNumber}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    <apex:inputText id="preferredContactNumber" value="{!preferredContactNumber}" size="25" required="true" />
                                </span>
                                <span class="section-a-input-spacing" style="width: 223pt;">
                                    <label for="alternateContactNumber">Alternate Contact #: </label>
                                    <apex:outputText value="{!alternateContactNumber}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    <apex:inputText id="alternateContactNumber" value="{!alternateContactNumber}" size="25" />
                                </span>
                                <span class="section-a-input-spacing" style="width: 419pt;">
                                    <label for="address">Address: </label>
                                    <apex:outputText value="{!address}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    <apex:inputText id="address" value="{!address}" size="72" required="true" />
                                </span>
                                <span class="section-a-input-spacing" style="width: 140pt;">
                                    <label for="apt">Apt: </label>
                                    <apex:outputText value="{!apt}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    <apex:inputText id="apt" value="{!apt}" size="20" />
                                </span>
                                <span class="section-a-input-spacing" style="width: 288pt;">
                                    <label for="city">City: </label>
                                    <apex:outputText value="{!city}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    <apex:inputText id="city" value="{!city}" size="50" required="true" />
                                </span>
                                <span class="section-a-input-spacing" style="width: 126pt; padding-left: 1pt;">
                                    <label for="state">State:</label>
                                    <apex:outputPanel rendered="{!isPdfMode}">
                                        <apex:outputText value="{!state}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                        <apex:selectList value="{!state}" size="1" required="true">
                                            <apex:selectOptions value="{!stateOptions}" />
                                        </apex:selectList>
                                    </apex:outputPanel>
                                </span>
                                <span class="section-a-input-spacing" style="width: 123pt;">
                                    <label for="zip">Zip: </label>
                                    <apex:outputText value="{!zip}" rendered="{!isPdfMode}" styleClass="field-highlighting" />
                                    <apex:inputText id="zip" value="{!zip}" size="20" required="true" />
                                </span>
                            </div>
                        </td>
                    </tr>
                    <tr class="section-a-table-row-two">
                        <td class="section-a-table-data-cell-two">
                            <div class="s3" style="text-align: left;">
                                <span class="apex-input-spacing">
                                    <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                        <apex:inputCheckbox id="techWillContact" value="{!techWillContact}" />
                                        <label for="techWillContact">Tech will contact you on the day of installation</label>
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!isPdfMode}">
                                        <apex:image html-name="techWillContactImage" url="{!IF(techWillContact, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                        />
                                        <label style="display: inline;" for="techWillContactImage">Tech will contact you on the day of installation</label>
                                    </apex:outputPanel>
                                </span>
                            </div>
                        </td>
                        <td class="section-a-table-data-cell-three">
                            <p class="s8 section-a-frontier-disclaimer">
                                Frontier may use the number provided above to make autodialed or prerecorded calls to me about sales promotions and/or my
                                services
                            </p>
                        </td>
                    </tr>
                    <tr class="section-a-table-row-three">
                        <td colspan="2">
                            <div class="s3" style="padding-top: 3pt; padding-left: 4pt; text-indent: 0pt; text-align: left; ">
                                <span class="apex-input-spacing">
                                    <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                        <apex:inputCheckbox id="existingCustomerChange" value="{!existingCustomerChange}" />
                                        <label for="existingCustomerChange">Existing Customer - Change</label>
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!isPdfMode}">
                                        <apex:image html-name="existingCustomerChangeImage" url="{!IF(existingCustomerChange, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                        />
                                        <label style="display: inline;" for="existingCustomerChangeImage">Existing Customer - Change</label>
                                    </apex:outputPanel>
                                </span>
                                <span class="apex-input-spacing">
                                    <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                        <apex:inputCheckbox id="m2m" value="{!m2m}" />
                                        <label for="m2m">M2M</label>
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!isPdfMode}">
                                        <apex:image html-name="m2mImage" url="{!IF(m2m, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                        />
                                        <label style="display: inline;" for="m2mImage">M2M</label>
                                    </apex:outputPanel>
                                </span>
                                <span class="apex-input-spacing">
                                    <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                        <apex:inputCheckbox id="oneYearTerm" value="{!oneYearTerm}" />
                                        <label for="oneYearTerm">1 Year Term</label>
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!isPdfMode}">
                                        <apex:image html-name="oneYearTermImage" url="{!IF(oneYearTerm, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                        />
                                        <label style="display: inline;" for="oneYearTermImage">1 Year Term</label>
                                    </apex:outputPanel>
                                </span>
                                <span class="apex-input-spacing">
                                    <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                        <apex:inputCheckbox id="lowIncomeOfferEnrolled" value="{!lowIncomeOfferEnrolled}" />
                                        <label for="lowIncomeOfferEnrolled">Low Income Offer Enrolled</label>
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!isPdfMode}">
                                        <apex:image html-name="lowIncomeOfferEnrolledImage" url="{!IF(lowIncomeOfferEnrolled, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                        />
                                        <label style="display: inline;" for="lowIncomeOfferEnrolledImage">Low Income Offer Enrolled</label>
                                    </apex:outputPanel>
                                </span>
                            </div>
                        </td>
                    </tr>
                    <tr class="section-title-table-row">
                        <td class="section-title-data-cell" colspan="2">
                            <p class="s7 section-header-text" style="display: inline-block; margin-right:80pt;">
                                Section B: Internet
                            </p>
                            <p class="s7 section-header-text" style="display: inline-block">
                                You may review additional internet service performance details at frontier.com/internetdisclosures
                            </p>
                        </td>

                    </tr>
                    <tr class="section-b-table-row-one">
                        <td class="section-b-table-data-cell-one" colspan="1">
                            <div class="s3" style="text-align: left; max-width: 286pt;">
                                <p class="s12" style="margin: 3pt;">Frontier Fiber Internet</p>
                                <div style="height: 46pt;">
                                    <span class="section-b-input-spacing">
                                        <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                            <apex:inputCheckbox id="fiber100" value="{!fiber100}" />
                                            <label for="fiber100">Fiber 100</label>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!isPdfMode}">
                                            <apex:image html-name="fiber100Image" url="{!IF(fiber100, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                            />
                                            <label style="display: inline;" for="fiber100Image">Fiber 100</label>
                                        </apex:outputPanel>
                                    </span>

                                    <span class="section-b-input-spacing">
                                        <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                            <apex:inputCheckbox id="fiber200" value="{!fiber200}" />
                                            <label for="fiber200">Fiber 200</label>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!isPdfMode}">
                                            <apex:image html-name="fiber200Image" url="{!IF(fiber200, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                            />
                                            <label style="display: inline;" for="fiber200Image">Fiber 200</label>
                                        </apex:outputPanel>
                                    </span>

                                    <span class="section-b-input-spacing">
                                        <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                            <apex:inputCheckbox id="fiber500" value="{!fiber500}" />
                                            <label for="fiber500">Fiber 500</label>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!isPdfMode}">
                                            <apex:image html-name="fiber500Image" url="{!IF(fiber500, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                            />
                                            <label style="display: inline;" for="fiber500Image">Fiber 500</label>
                                        </apex:outputPanel>
                                    </span>

                                    <span class="section-b-input-spacing">
                                        <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                            <apex:inputCheckbox id="fiber1Gig" value="{!fiber1Gig}" />
                                            <label for="fiber1Gig">Fiber 1 Gig</label>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!isPdfMode}">
                                            <apex:image html-name="fiber1GigImage" url="{!IF(fiber1Gig, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                            />
                                            <label style="display: inline;" for="fiber1GigImage">Fiber 1 Gig</label>
                                        </apex:outputPanel>
                                    </span>

                                    <span class="section-b-input-spacing">
                                        <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                            <apex:inputCheckbox id="fiber2Gig" value="{!fiber2Gig}" />
                                            <label for="fiber2Gig">Fiber 2 Gig</label>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!isPdfMode}">
                                            <apex:image html-name="fiber2GigImage" url="{!IF(fiber2Gig, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                            />
                                            <label style="display: inline;" for="fiber2GigImage">Fiber 2 Gig</label>
                                        </apex:outputPanel>
                                    </span>

                                    <span class="section-b-input-spacing">
                                        <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                            <apex:inputCheckbox id="fiber5Gig" value="{!fiber5Gig}" />
                                            <label for="fiber5Gig">Fiber 5 Gig</label>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!isPdfMode}">
                                            <apex:image html-name="fiber5GigImage" url="{!IF(fiber5Gig, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                            />
                                            <label style="display: inline;" for="fiber5GigImage">Fiber 5 Gig</label>
                                        </apex:outputPanel>
                                    </span>

                                    <span class="section-b-input-spacing">
                                        <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                            <apex:inputCheckbox id="fiber7Gig" value="{!fiber7Gig}" />
                                            <label for="fiber7Gig">Fiber 7 Gig</label>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!isPdfMode}">
                                            <apex:image html-name="fiber7GigImage" url="{!IF(fiber7Gig, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                            />
                                            <label style="display: inline;" for="fiber7GigImage">Fiber 7 Gig</label>
                                        </apex:outputPanel>
                                    </span>
                                </div>
                                <p class="s6" style="padding-left: 4pt; padding-right: 81pt; text-indent: 0pt; text-align: left; width: 200pt;">
                                    For 200, 500, 1 Gig, 5 Gig, and 7 Gig, $100 prorated early termination fee applies if cancelled months 1-12.
                                </p>
                            </div>
                        </td>
                        <td colspan="1">
                            <div class="s9" style="text-align: left; max-width: 287.5pt; height:79pt;">
                                <span class="section-b-input-spacing" style="margin-top 2pt;">
                                    <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                        <apex:inputCheckbox id="frontierInternet" value="{!frontierInternet}" />
                                        <label for="frontierInternet">
                                            <span class="s12">Frontier Internet</span>
                                        </label>
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!isPdfMode}">
                                        <apex:image html-name="frontierInternetImage" url="{!IF(frontierInternet, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                        />
                                        <label style="display: inline;" for="frontierInternetImage">
                                            <span class="s12">Frontier Internet</span>
                                        </label>
                                    </apex:outputPanel>
                                </span>

                                <p style="padding-top: 2pt; text-indent: 0pt; text-align: left;">
                                    <br />
                                </p>
                                <p class="s12" style="padding-left: 16pt; text-indent: 0pt; text-align: left;">
                                    Streaming capabilities
                                </p>
                                <p class="s15" style="padding-left: 16pt; text-indent: 0pt; text-align: left;">
                                    Your address qualifies for the following streaming capabilities:
                                </p>

                                <div class="s3" style="padding-left: 4pt; max-width: 287.5pt;">
                                    <span class="section-b-two-input-spacing">
                                        <apex:outputPanel rendered="{!NOT(isPdfMode)}" style="font-size:x-small;">
                                            <apex:inputCheckbox id="checkEmailBankSocial" value="{!checkEmailBankSocial}" />
                                            <label for="checkEmailBankSocial">Check email, bank online, scroll through social media</label>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!isPdfMode}" style="font-size:x-small;">
                                            <apex:image html-name="checkEmailBankSocialImage" url="{!IF(checkEmailBankSocial, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                            />
                                            <label style="display: inline;" for="checkEmailBankSocialImage">Check email, bank online, scroll through social media</label>
                                        </apex:outputPanel>
                                    </span>

                                    <span class="section-b-two-input-spacing" style="font-size:x-small;">
                                        <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                            <apex:inputCheckbox id="streamMoviesGames" value="{!streamMoviesGames}" />
                                            <label for="streamMoviesGames">Stream your favorite movies and TV shows, or play games online</label>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!isPdfMode}">
                                            <apex:image html-name="streamMoviesGamesImage" url="{!IF(streamMoviesGames, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                            />
                                            <label style="display: inline;" for="streamMoviesGamesImage">Stream your favorite movies and TV shows, or play games online</label>
                                        </apex:outputPanel>
                                    </span>

                                    <span class="section-b-two-input-spacing" style="font-size:x-small;">
                                        <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                            <apex:inputCheckbox id="streamMultiDevices4K" value="{!streamMultiDevices4K}" />
                                            <label for="streamMultiDevices4K">Watch streaming video on multiple devices, plus stream incredible 4K video</label>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!isPdfMode}">
                                            <apex:image html-name="streamMultiDevices4KImage" url="{!IF(streamMultiDevices4K, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                            />
                                            <label style="display: inline;" for="streamMultiDevices4KImage">Watch streaming video on multiple devices, plus stream incredible 4K video</label>
                                        </apex:outputPanel>
                                    </span>
                                </div>
                            </div>
                        </td>
                    </tr>
                    <!-- Section C -->
                    <tr class="section-title-table-row">
                        <td class="section-title-data-cell" colspan="2">
                            <p class="s7" style="padding-top: 3pt; padding-left: 4pt; text-indent: 0pt; text-align: left; ">
                                Section C: Additional Services
                            </p>
                        </td>
                    </tr>
                    <tr class="section-c-table-row-one">
                        <td colspan="2">
                            <div class="s3" style="text-align: left;">
                                <span class="apex-input-spacing">
                                    <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                        <apex:inputCheckbox id="youTubeTvBaseEnglish" value="{!youTubeTvBaseEnglish}" />
                                        <label for="youTubeTvBaseEnglish">YouTube TV Base (English)</label>
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!isPdfMode}">
                                        <apex:image html-name="youTubeTvBaseEnglishImage" url="{!IF(youTubeTvBaseEnglish, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                        />
                                        <label style="display: inline;" for="youTubeTvBaseEnglishImage">YouTube TV Base (English)</label>
                                    </apex:outputPanel>
                                </span>
                                <span class="apex-input-spacing">
                                    <apex:outputPanel rendered="{!NOT(isPdfMode)}">
                                        <apex:inputCheckbox id="youTubeTvSpanish" value="{!youTubeTvSpanish}" />
                                        <label for="youTubeTvSpanish">YouTube TV Spanish</label>
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!isPdfMode}">
                                        <apex:image html-name="youTubeTvSpanishImage" url="{!IF(youTubeTvSpanish, URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_true.png'), URLFOR($Resource.CSAFormPageResources, 'CSAFormPageResources/images/checkbox_false.png'))}"
                                        />
                                        <label style="display: inline;" for="youTubeTvSpanishImage">YouTube TV Spanish</label>
                                    </apex:outputPanel>
