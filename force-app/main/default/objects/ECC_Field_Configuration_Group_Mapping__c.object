    <externalSharingModel>Private</externalSharingModel>
    <label>ECC Field Configuration Group Mapping</label>
    <nameField>
        <displayFormat>ECCFCG-{00000}</displayFormat>
        <label>ECC Field Configuration Group Number</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>ECC Field Configuration Group Mappings</pluralLabel>
    <searchLayouts></searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <visibility>Public</visibility>
