<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_Must_Be_After_Start_Date</fullName>
    <active>true</active>
    <errorConditionFormula>POE_End_Date__c &lt;= POE_Start_Date__c</errorConditionFormula>
    <errorDisplayField>POE_End_Date__c</errorDisplayField>
    <errorMessage>The End Date cannot be before the Start Date.</errorMessage>
</ValidationRule>
