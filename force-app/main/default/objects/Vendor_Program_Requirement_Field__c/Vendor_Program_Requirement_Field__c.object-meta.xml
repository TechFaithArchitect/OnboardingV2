    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>Private</externalSharingModel>
    <label>Vendor Program Requirement Field</label>
    <nameField>
        <displayFormat>VPRF-{0000}</displayFormat>
        <label>Vendor Program Requirement Field Name</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Vendor Program Requirement Fields</pluralLabel>
    <searchLayouts />
    <sharingModel>ReadWrite</sharingModel>
    <visibility>Public</visibility>
