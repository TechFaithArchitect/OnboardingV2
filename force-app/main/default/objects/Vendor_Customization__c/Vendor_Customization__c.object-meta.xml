<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Versioned vendor program configuration chosen during onboarding (child versions inherit from parent versions; drives requirement sets, status rules, and wizard selection).</description>
    <enableActivities>true</enableActivities>
    <enableReports>true</enableReports>
    <enableSharing>true</enableSharing>
    <enableHistory>true</enableHistory>
    <label>Vendor Program</label>
    <pluralLabel>Vendor Programs</pluralLabel>
    <nameField>
        <label>Vendor Program Name</label>
        <type>Text</type>
    </nameField>
    <deploymentStatus>Deployed</deploymentStatus>
    <!-- Master-detail to Vendor; sharing inherited from parent -->
    <sharingModel>ControlledByParent</sharingModel>
    <visibility>Public</visibility>
</CustomObject>
