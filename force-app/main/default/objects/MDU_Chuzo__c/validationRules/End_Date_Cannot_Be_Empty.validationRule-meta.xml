<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_Cannot_Be_Empty</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISBLANK( Start_Date__c ), NOT(ISBLANK( End_Date__c )))</errorConditionFormula>
    <errorMessage>An End Date can&apos;t be set if the MDU doesn&apos;t have a Start Date.</errorMessage>
</ValidationRule>
