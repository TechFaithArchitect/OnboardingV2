<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status__c</fullName>
    <description>Status of an Event [PER-4044]</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISBLANK(End_Date__c)) &amp;&amp; End_Date__c &lt; TODAY(), &quot;Archived&quot;, &quot;Active&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Check if the event has an end date and if that date has already passed</inlineHelpText>
    <label>Status</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
