<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Training_System_Uniqueness</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
  ISNEW(),
  NOT(ISBLANK(Name)),
  CASESAFEID(Name) = CASESAFEID(PRIORVALUE(Name))
  )</errorConditionFormula>
    <errorMessage>Training System must be unique. The Training System entered already exists.</errorMessage>
</ValidationRule>
