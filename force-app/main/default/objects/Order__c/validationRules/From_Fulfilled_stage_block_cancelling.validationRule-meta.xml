<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>From_Fulfilled_stage_block_cancelling</fullName>
    <active>true</active>
    <errorConditionFormula>AND(is_Fulfilled__c = TRUE , ISPICKVAL( Stage__c , &quot;Cancelled&quot;) )</errorConditionFormula>
    <errorMessage>Cannot cancel the fulfilled order</errorMessage>
</ValidationRule>
