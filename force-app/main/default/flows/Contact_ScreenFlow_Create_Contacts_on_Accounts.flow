    <formulas>
        <name>FormattedPhoneNumber</name>
        <dataType>String</dataType>
        <expression>IF(
    OR(
        /* Matches +1XXXXXXXXXX (Correct Format) */
        REGEX({!PhoneNumber}, &quot;^\\+1[2-9]\\d{2}\\d{7}$&quot;),

        /* Matches 1XXXXXXXXXX (Missing &quot;+&quot;, Will Fix) */
        REGEX({!PhoneNumber}, &quot;^1[2-9]\\d{2}\\d{7}$&quot;),

        /* Matches various formats with spaces, dashes, dots, or parentheses */
        REGEX({!PhoneNumber}, &quot;^\\+1\\s?\\(?[2-9]\\d{2}\\)?[-.\\s]?\\d{3}[-.\\s]?\\d{4}$&quot;),
        REGEX({!PhoneNumber}, &quot;^1\\s?\\(?[2-9]\\d{2}\\)?[-.\\s]?\\d{3}[-.\\s]?\\d{4}$&quot;),
        REGEX({!PhoneNumber}, &quot;^\\(?[2-9]\\d{2}\\)?[-.\\s]?\\d{3}[-.\\s]?\\d{4}$&quot;)
    ),
    &quot;+1 &quot; &amp; RIGHT(
        SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(
            {!PhoneNumber}, &quot; &quot;, &quot;&quot;), &quot;-&quot;, &quot;&quot;), &quot;(&quot;, &quot;&quot;), &quot;)&quot;, &quot;&quot;), &quot;.&quot;, &quot;&quot;),
        10
    ),
    &quot;&quot; /* Returns blank if invalid */
)</expression>
    </formulas>
